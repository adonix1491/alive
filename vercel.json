{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        },
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "api/probe.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/probe",
            "dest": "/api/probe.ts"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.ts"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
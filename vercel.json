{
    "version": 2,
    "builds": [
        {
            "src": "api/app.ts",
            "use": "@vercel/node"
        },
        {
            "src": "AliveApp/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/app.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/AliveApp/$1"
        }
    ]
}
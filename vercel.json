{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/probe",
            "destination": "/api/probe.ts"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/((?!api/|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}